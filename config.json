{
  "version": 2,
  "directory": "%Downloads%",
  "rules": [
    {
      "name": "EKI file automation",
      "conditions": [
        {
          "property": "name",
          "regexp": "2019.*[\\d]{10}\\-(Kontoauszug|Einzelanzeige)\\.pdf$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%\\Service\\Finanzen\\EKI\\Dokumente\\2019\\{{name}}"
      },
      "id": "move-to-eki-2019-documents"
    },
    {
      "name": "EKI file automation",
      "conditions": [
        {
          "property": "name",
          "regexp": "^2020.*[\\d]{14}\\-(Kontoauszug|Einzelanzeige|Kontoabschluss_auszug|Zins-_Kapitalausweis|Private Banking Report)\\.pdf$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%\\Service\\Finanzen\\EKI\\Dokumente\\2020\\{{name}}"
      },
      "id": "move-to-eki-2020-documents"
    },
    {
      "name": "EKI file automation",
      "conditions": [
        {
          "property": "name",
          "regexp": "^2021.*[\\d]{14}\\-(Kontoauszug|Einzelanzeige|Kontoabschluss_auszug|Zins-_Kapitalausweis|Private Banking Report)\\.pdf$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%\\Service\\Finanzen\\EKI\\Dokumente\\2021\\{{name}}"
      },
      "id": "move-to-eki-2021-documents"
    },
    {
      "name": "BEKB file automation",
      "parseFileName": "(?<portfolio>\\d+)-(?<year>\\d\\d\\d\\d)(?<month>\\d\\d)(?<day>\\d\\d)-(?<subject>[A-Za-z]+)-(?<nr>\\d*)\\.",
      "conditions": [
        {
          "property": "portfolio",
          "regexp": "^(42593270865|42925041863|42488142633)$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Finanzen/BEKB/{{year}}/{{subject}}/{{name}}"
      },
      "id": "move-to-bekb-yearly-documents"
    },
    {
      "name": "BEKB legacy automation",
      "parseFileName": "(?<subject>[A-Za-z]+)_(?<legacy>[A-Za-z]+)_(?<portfolio>\\d+)_(?<year>\\d\\d\\d\\d)(?<month>\\d\\d)(?<day>\\d\\d)\\.",
      "conditions": [
        {
          "property": "legacy",
          "regexp": "^bekb$",
          "@type": "FilePattern"
        },
        {
          "property": "portfolio",
          "regexp": "^(42593270865|42925041863|42488142633)$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Finanzen/BEKB/{{year}}/{{subject}}/{{portfolio}}-{{year}}{{month}}{{day}}-{{subject}}-00{{ext}}"
      },
      "id": "legacy-move-to-bekb-yearly-documents"
    },
    {
      "name": "Viseca file automation",
      "parseFileName": "(?<subject>[A-Za-z]+)_(?<year>\\d\\d\\d\\d)(?<month>\\d\\d)(?<day>\\d\\d)_.*_(?<portfolio>\\d+)_(?<nr>\\d*)\\.",
      "conditions": [
        {
          "property": "subject",
          "regexp": "^visebpp$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Finanzen/Viseca/{{year}}/{{name}}"
      },
      "id": "move-to-viseca-yearly-documents"
    },
    {
      "name": "Visana yearly file automation",
      "parseFileName": "(?<subject>[A-Za-z]+)(?<nr>\\d*)_(?<portfolio>\\d+)_(?<year>\\d\\d\\d\\d)(?<month>\\d\\d)(?<day>\\d\\d)\\.",
      "conditions": [
        {
          "property": "subject",
          "regexp": "^((?!Steuernachweis).)*$",
          "@type": "FilePattern"
        },
        {
          "property": "portfolio",
          "regexp": "^105340865$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Krankenkasse/Visana/{{year}}/{{subject}}/{{name}}"
      },
      "id": "move-to-visana-yearly-tax-folder"
    },
    {
      "name": "Visana Steuernachweis yearly file automation",
      "parseFileName": "(?<subject>[A-Za-z]+)_(?<portfolio>\\d+)_(?<year>\\d\\d\\d\\d)(?<month>\\d\\d)(?<day>\\d\\d)\\.",
      "conditions": [
        {
          "property": "subject",
          "regexp": "^Steuernachweis$",
          "@type": "FilePattern"
        },
        {
          "property": "portfolio",
          "regexp": "^105340865$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Krankenkasse/Visana/{{year-1}}/{{subject}}/{{name}}"
      },
      "id": "move-to-visana-yearly-subject-folder"
    },
    {
      "name": "AXA yearly file automation",
      "parseFileName": "(?<subject>[A-Za-z]+)_(?<portfolio>\\d+)_(?<year>\\d\\d\\d\\d)-(?<month>\\d\\d)-(?<day>\\d\\d)\\.",
      "conditions": [
        {
          "property": "portfolio",
          "regexp": "^18754653$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/Service/Versicherung/AXA Winterthur/{{year}}/{{name}}"
      },
      "id": "move-to-axa-yearly-document"
    },
    {
      "name": "Test automation",
      "parseFileName": "(?<year>\\d\\d\\d\\d)-(?<month>\\d\\d)-(?<day>\\d\\d)",
      "conditions": [
        {
          "property": "name",
          "regexp": "^test.*\\.txt$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%/{{year}}//{{name}}",
        "options": {
          "clobber": false,
          "mkdirp": true
        }
      },
      "id": "test-id"
    }
  ]
}