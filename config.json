{
  "version": 2,
  "directory": "%Downloads%",
  "rules": [
    {
      "name": "EKI file automation",
      "conditions": [
        {
          "property": "name",
          "regexp": "2019.*[\\d]{10}\\-(Kontoauszug|Einzelanzeige)\\.pdf$",
          "@type": "FilePattern"
        }
      ],
      "action": {
        "@type": "MoveToAction",
        "destination": "%GoogleDrive%\\Service\\Finanzen\\EKI\\Dokumente\\2019"
      },
      "id": "move-to-eki-2019-documents"
    },
